parameters:
  bootstrapFiles:
    - ../vendor/autoload.php
  excludes_analyse:
    - ./src/Readme
  level: max
  paths:
    - ./src
    - ../src
  reportMaybesInMethodSignatures: false

includes:
  - ./vendor/phpstan/phpstan/conf/bleedingEdge.neon
  - ./vendor/phpstan/phpstan-strict-rules/rules.neon
